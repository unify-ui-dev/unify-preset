{
	"$schema": "https://biomejs.dev/schemas/1.5.1/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"formatter": {
		"enabled": true,
		"formatWithErrors": false,
		"indentStyle": "tab",
		"ignore": []
	},
	"files":{
		"ignore":[
			"node_modules/**",
			"/node_modules/**",
			"/dist/**/*.js"
		]
	}
}
