{
  "name": "@unifydev/unify-variant",
  "version": "0.0.0-beta",
  "description": "UnifyUI Variants",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "rimraf dist && unbuild",
    "format": "biome format --write ./src"
  },
  "files": [
    "dist",
    "favicon-dark.png",
    "README.md",
    "package.json",
    "!.gitignore",
    "!tsconfig.json",
    "!biome.js"
  ],
  "author": "Johnkat MJ",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.6.2",
    "@types/node": "^20.11.27",
    "@unocss/reset": "^0.58.6",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "unbuild": "^2.0.0",
    "unocss": "^0.58.6"
  }
}
